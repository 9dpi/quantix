# Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to Git!

# Distributed System Configuration
INTERNAL_AUTH_KEY=your-32-character-secret-key-here
MVP_API_URL=http://localhost:3001/api/v1/internal/signals
TEST_PORT=3001

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Database Direct Connection (Optional)
DB_HOST=your-db-host.supabase.com
DB_NAME=postgres
DB_USER=postgres.your-project-id
DB_PASSWORD=your-db-password
DB_PORT=6543

# Telegram Bot
TELEGRAM_TOKEN=your-bot-token-here
TELEGRAM_CHAT_ID=your-chat-id-here

# External APIs
ALPHA_VANTAGE_KEY=your-alpha-vantage-key-here

# Production URLs (Update for deployment)
# MVP_API_URL=https://your-production-mvp.com/api/v1/internal/signals
